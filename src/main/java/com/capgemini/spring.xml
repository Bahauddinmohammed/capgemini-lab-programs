<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
        
        
         <bean id="sbu" class="com.capgemini.SBU">
    <property name="sbuCode" value="101"></property>
    <property name="sbuName" value="Akash"></property>
    <property name="sbuHead" value="Chris"></property>
       <property name="empList">
       <list>
          <ref bean="employee" />
       </list>
       </property> 
        </bean>
        
        
        <bean id="employee" class="com.capgemini.Employee">
    <property name="employeeId" value="11"></property>
    <property name="employeeName" value="mahesh"></property>
    <property name="salary" value="40000.00"></property>
    
    </bean>
        </beans>